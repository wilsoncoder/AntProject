<?xml version="1.0" encoding="UTF-8"?>
<project default="run" name="wilson's Demo project on ant">
	<target name="run" depends="compile">
		<java classname="com.wil.code.Demo">
			<classpath path="stage" />
		</java>
	</target>
	
	<target name="compile">
		<javac includeantruntime="false" srcdir="./src" destdir="stage" />
	</target>

</project>